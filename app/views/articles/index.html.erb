<div class="container">
  <h1>Articles</h1>
  <% @articles.each do |article| %>
    <h2> <%= link_to article.title, article_path(article) %></h2>
    <div class="delete"> <%= link_to "delete", article_path(article),
        method: :delete,
        data: { confirm: "Are you sure?" } %> </div>
  <% end %>
</div>
